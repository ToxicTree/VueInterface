{
  "name": "vueinterface",
  "version": "2.0.0",
  "description": "Basic interface for Vue components",
  "main": "index.php",
  "scripts": {
    "install": " cp -n './components.js.example' './components.js'",
    "sass": "node-sass src/main.scss dist/build.css",
    "browserify": "cross-env NODE_ENV=production browserify -g envify -e src/main.js -o dist/build.js",
    "build": "npm-run-all --parallel sass browserify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserify": {
    "transform": [
      "vueify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ToxicTree/VueInterface.git"
  },
  "author": "Jonathan (https://github.com/ToxicTree)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ToxicTree/VueInterface/issues"
  },
  "homepage": "https://github.com/ToxicTree/VueInterface#readme",
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-preset-es2015": "^6.16.0",
    "bootstrap-sass": "^3.3.7",
    "browserify": "^13.1.0",
    "cross-env": "^3.1.3",
    "envify": "^3.4.1",
    "node-sass": "^3.10.1",
    "npm-run-all": "^3.1.1",
    "vue": "^2.0.3",
    "vueify": "^9.2.4"
  }
}
